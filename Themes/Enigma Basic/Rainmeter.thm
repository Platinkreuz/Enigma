[Rainmeter]

;-----------------------
; META

[Enigma\Options]
Active=1
WindowX=50%
WindowY=50%
AnchorX=50%
AnchorY=50%
LoadOrder=1

;-----------------------
; BARS

[Enigma\Sidebar]
Active=0
WindowX=#WORKAREAX#
WindowY=#WORKAREAY#
LoadOrder=-1
AlwaysOnTop=-2

[Enigma\Sidebar\_Sidebar2]
Active=1
WindowX=(#WORKAREAX#+#WORKAREAWIDTH#-200)
WindowY=#WORKAREAY#
LoadOrder=-1
AlwaysOnTop=-2

[Enigma\Taskbar]
Active=0
WindowX=#WORKAREAX#
WindowY=#WORKAREAY#
LoadOrder=-1
AlwaysOnTop=-2

[Enigma\Taskbar\_Taskbar2]
Active=1
WindowX=#WORKAREAX#
WindowY=(#WORKAREAY#+#WORKAREAHEIGHT#-44)
LoadOrder=-1
AlwaysOnTop=-2

;-----------------------
; SIDEBAR

[Enigma\Sidebar\Clock]
